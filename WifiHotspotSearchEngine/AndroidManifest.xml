<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.allendroid.whse"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/ic_launcher_wifi" android:label="@string/app_name">
        <activity android:name=".WifiHotspotSE"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity android:name=".MenuActivity" 
				android:theme="@style/Theme.Translucent"/>
	    <activity android:name=".UploadAccountActivity" 
	    		android:theme="@style/Theme.Menu"/>
	    <activity android:name=".NearbySpotListActivity" 
	    		android:theme="@style/Theme.Menu"/>
	    <activity android:name=".MapViewActivity" 
	    		android:theme="@style/Theme.Menu"/>
	<uses-library android:name="com.google.android.maps" />
    </application>
    <uses-sdk android:minSdkVersion="4" />

<uses-permission android:name="android.permission.INTERNET"></uses-permission>
<uses-permission android:name="android.permission.ACCESS_GPS"> </uses-permission> 
<uses-permission android:name="android.permission.ACCESS_LOCATION"> </uses-permission>
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"> </uses-permission> 
